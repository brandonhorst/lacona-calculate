{
  "name": "lacona-calculate",
  "version": "1.0.3",
  "description": "Lacona Command for doing arithmetic and unit conversions",
  "lacona": {
    "title": "Calculator",
    "customPreferences": true,
    "description": "Solve basic math problems and do unit conversions."
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonhorst/lacona-calculate.git"
  },
  "keywords": [
    "lacona",
    "calculate"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonhorst/lacona-calculate/issues"
  },
  "homepage": "https://github.com/brandonhorst/lacona-calculate",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "elliptical": "^2.0.0",
    "lacona-api": "^1.4.1",
    "lacona-phrases": "^1.1.0",
    "lodash": "^4.16.4",
    "mathjs": "^3.6.0",
    "rxjs": "^5.0.0-rc.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  }
}
